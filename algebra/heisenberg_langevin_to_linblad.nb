Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"the", " ", "Schrodinger", " ", "picture", " ", "operators"}], 
    ",", " ", 
    RowBox[{
     RowBox[{
     "or", " ", "equivalently", " ", "the", " ", "Heisenberg", " ", "ops", 
      " ", "at", " ", "t"}], "=", "t0"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Sigma]0", "=", 
     RowBox[{"PauliMatrix", "[", "0", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Sigma]x", "=", 
     RowBox[{"PauliMatrix", "[", "1", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Sigma]y", "=", 
     RowBox[{"PauliMatrix", "[", "2", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Sigma]z", "=", 
     RowBox[{"PauliMatrix", "[", "3", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Sigma]p", "=", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"(", 
       RowBox[{"\[Sigma]x", "+", 
        RowBox[{"\[ImaginaryI]", " ", "\[Sigma]y"}]}], ")"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Sigma]m", "=", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"(", 
       RowBox[{"\[Sigma]x", "-", 
        RowBox[{"\[ImaginaryI]", " ", "\[Sigma]y"}]}], ")"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"a", "=", "\[Sigma]m"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"adag", "=", "\[Sigma]p"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"dag", "[", "z_", "]"}], "=", 
     RowBox[{"ConjugateTranspose", "[", "z", "]"}]}], ";"}]}]}]], "Input",Expr\
essionUUID->"4801cff4-5689-49be-b9dd-3a6354e42608"]
