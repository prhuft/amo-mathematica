Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"decay", "[", "t_", "]"}], "=", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"t", "<", "0"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "t"}], "/", "\[Tau]"}], "]"}], ",", 
        RowBox[{"t", ">=", "0"}]}], "}"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"pulse", "[", "t_", "]"}], "=", 
   RowBox[{"Exp", "[", 
    FractionBox[
     RowBox[{"-", 
      SuperscriptBox["t", "2"]}], 
     SuperscriptBox["tp", "2"]], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Assuming", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"tp", ">", "0"}], "&&", 
    RowBox[{"\[Tau]", ">", "0"}], "&&", 
    RowBox[{"t", "\[Element]", "Reals"}]}], ",", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"decay", "[", "y", "]"}], 
      RowBox[{"pulse", "[", 
       RowBox[{"t", "-", "y"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"y", ",", 
       RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}]}], "]"}]}], 
