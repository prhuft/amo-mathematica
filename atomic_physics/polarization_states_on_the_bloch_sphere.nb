           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Alpha]"}]], 
          SuperscriptBox[
           RowBox[{"Sin", "[", "\[Beta]", "]"}], "2"]}]}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Alpha]"}]]}], ")"}], 
         RowBox[{"Cos", "[", "\[Beta]", "]"}], 
         RowBox[{"Sin", "[", "\[Beta]", "]"}]}]},
       {
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Alpha]"}]]}], ")"}], 
         RowBox[{"Cos", "[", "\[Beta]", "]"}], 
         RowBox[{"Sin", "[", "\[Beta]", "]"}]}], 
        RowBox[{
         RowBox[{
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Alpha]"}]], 
          SuperscriptBox[
           RowBox[{"Cos", "[", "\[Beta]", "]"}], "2"]}], "+", 
         SuperscriptBox[
          RowBox[{"Sin", "[", "\[Beta]", "]"}], "2"]}]}
      }], ")"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"\[Alpha]", " ", "=", " ", 
     RowBox[{"retardance", " ", "[", "rad", "]"}]}], ",", " ", 
    RowBox[{
    "real", " ", "space", " ", "angle", " ", "of", " ", "waveplate", " ", 
     "coordinate", " ", 
     RowBox[{"rotation", " ", "[", "rad", "]"}]}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
