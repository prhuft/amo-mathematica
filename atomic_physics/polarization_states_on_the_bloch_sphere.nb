   RowBox[{
    RowBox[{"waveplate", "[", 
     RowBox[{"\[Alpha]_", ",", "\[Beta]_"}], "]"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SuperscriptBox[
          RowBox[{"Cos", "[", "\[Beta]", "]"}], "2"], "+", 
