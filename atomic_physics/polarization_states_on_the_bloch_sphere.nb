Cell["\<\
a nice derivation of the transformation of the Jones vectors and operators to \
the Bloch sphere picture is given in David Nadlinger\[CloseCurlyQuote]s \
Masters thesis. below are the results.\
\>", "Text",
 CellChangeTimes->{{3.846673401655239*^9, 
  3.846673475920544*^9}},ExpressionUUID->"338f8fc0-3de6-4b86-8fce-\
33e1031116a0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"waveplate", "[", 
    RowBox[{"\[Alpha]_", ",", "\[Beta]_"}], "]"}], ":=", 
   RowBox[{"(", GridBox[{
      {
       RowBox[{
        SuperscriptBox[
         RowBox[{"Cos", "[", 
          RowBox[{"\[Alpha]", "/", "2"}], "]"}], "2"], "-", 
        RowBox[{
         RowBox[{"Cos", "[", 
          RowBox[{"4", "\[Beta]"}], "]"}], 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           RowBox[{"\[Alpha]", "/", "2"}], "]"}], "2"]}]}], 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{"2", "\[Beta]"}], "]"}], 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], 
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"4", "\[Beta]"}], "]"}], 
        SuperscriptBox[
         RowBox[{"Sin", "[", 
          RowBox[{"\[Alpha]", "/", "2"}], "]"}], "2"]}]},
      {
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Cos", "[", 
          RowBox[{"2", "\[Beta]"}], "]"}]}], 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], 
       RowBox[{"Cos", "[", "\[Alpha]", "]"}], 
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"2", "\[Beta]"}], "]"}], 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}]},
      {
