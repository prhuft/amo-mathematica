Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"\[CapitalDelta]a", ",", "\[CapitalDelta]c", ",", "g", ",", "n"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hjc", "=", 
   RowBox[{"(", GridBox[{
      {
       RowBox[{"\[CapitalDelta]c", " ", "n"}], 
       RowBox[{"g", " ", 
        SqrtBox["n"]}]},
      {
       RowBox[{"g", " ", 
        SqrtBox["n"]}], 
       RowBox[{
        RowBox[{"\[CapitalDelta]c", " ", 
         RowBox[{"(", 
          RowBox[{"n", "-", "1"}], ")"}]}], "+", "\[CapitalDelta]a"}]}
     }], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"{", 
   RowBox[{"evals", ",", "evecs"}], "}"}], "=", 
  RowBox[{"Eigensystem", "[", "Hjc", "]"}]}]}], "Input",
