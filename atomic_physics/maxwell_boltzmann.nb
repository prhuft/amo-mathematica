Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "prob", " ", "of", " ", "speed", " ", "v", " ", "at", " ", "temp", " ", 
    "T"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"MaxBoltzDist", "[", 
      RowBox[{"T_", ",", "v_"}], "]"}], ":=", 
     RowBox[{"PDF", "[", 
      RowBox[{
       RowBox[{"MaxwellDistribution", "[", 
        SqrtBox[
         FractionBox[
          RowBox[{"kB", " ", "T"}], "mRb"]], "]"}], ",", "v"}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"MaxBoltzSpeedSample", "[", 
      RowBox[{"T_", ",", "speedband_", ",", "samples_"}], "]"}], ":=", 
     RowBox[{"Module", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"fmax", ",", "y", ",", "v", ",", 
         RowBox[{"vdomain", "=", "speedband"}], ",", 
         RowBox[{"f", "=", "0"}], ",", 
         RowBox[{"flist", "=", 
          RowBox[{"{", "}"}]}], ",", 
         RowBox[{"vlist", "=", 
          RowBox[{"{", "}"}]}], ",", 
         RowBox[{"ylist", "=", 
          RowBox[{"{", "}"}]}], ",", 
         RowBox[{"samps", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"fmax", "=", 
         RowBox[{"MaxBoltzDist", "[", 
          RowBox[{"T", ",", 
           SqrtBox[
            FractionBox[
             RowBox[{"kB", " ", "T"}], "mRb"]]}], "]"}]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"While", " ", "[", 
         RowBox[{
          RowBox[{"samps", "<", "samples"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"v", " ", "=", " ", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"vdomain", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "-", 
               RowBox[{"vdomain", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], ")"}], 
             RowBox[{"RandomReal", "[", "]"}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"f", " ", "=", 
            RowBox[{"MaxBoltzDist", "[", 
             RowBox[{"T", ",", "v"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"y", "=", 
            RowBox[{"fmax", " ", 
             RowBox[{"RandomReal", "[", "]"}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"y", "\[LessEqual]", "f"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"ylist", ",", "y"}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
                RowBox[{"prob", " ", "that", " ", "y"}], "<=", 
                RowBox[{
                "f", " ", "is", " ", "prob", " ", "that", " ", "speed", " ", 
                 "is", " ", "measured"}]}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"vlist", ",", "v"}], "]"}], ";", 
              RowBox[{"(*", "speeds", "*)"}], "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"flist", ",", "f"}], "]"}], ";", 
              RowBox[{"(*", "probabilities", "*)"}], "\[IndentingNewLine]", 
              RowBox[{"samps", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
           ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"vlist", ",", "ylist", ",", "flist"}], "}"}]}]}], 
      "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"MaxBoltzVelocitySample", "[", 
      RowBox[{"T_", ",", "speedband_", ",", "samples_"}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"velocityList", "=", 
          RowBox[{"{", "}"}]}], ",", "ex", ",", "ey", ",", "ez", ",", "v", 
         ",", "A"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"n", "=", "1"}], ",", 
          RowBox[{"n", "<", 
           RowBox[{"samples", "+", "1"}]}], ",", 
          RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"ex", "=", 
            RowBox[{
             RowBox[{"2", " ", 
              RowBox[{"RandomReal", "[", "]"}]}], "-", "1"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"ey", "=", 
            RowBox[{
             RowBox[{"2", " ", 
              RowBox[{"RandomReal", "[", "]"}]}], "-", "1"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"ez", "=", 
            RowBox[{
             RowBox[{"2", " ", 
              RowBox[{"RandomReal", "[", "]"}]}], "-", "1"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"v", "=", 
            RowBox[{
             RowBox[{"MaxBoltzSpeedSample", "[", 
              RowBox[{"T", ",", "speedband", ",", "1"}], "]"}], "[", 
             RowBox[{"[", 
              RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"A", "=", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["ex", "2"], "+", 
              SuperscriptBox["ey", "2"], "+", 
              SuperscriptBox["ez", "2"]}]]}], ";", "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"velocityList", ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"ex", " ", 
                RowBox[{"v", "/", "A"}]}], ",", 
               RowBox[{"ey", " ", 
                RowBox[{"v", "/", "A"}]}], ",", 
               RowBox[{"ez", " ", 
                RowBox[{"v", "/", "A"}]}]}], "}"}]}], "]"}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        "velocityList"}]}], "\[IndentingNewLine]", "]"}]}], 
    ";"}]}]}]], "Input",
