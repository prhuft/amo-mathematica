    FractionBox["1", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"2", "Ccoop"}]}], ")"}], "*", 
      RowBox[{"conj", "[", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
