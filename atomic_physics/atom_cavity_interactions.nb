Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "=", 
   RowBox[{"2", "\[Pi]", " ", "*", "12*^6"}]}], ";", " ", 
  RowBox[{"(*", 
   RowBox[{"vacuum", " ", "Rabi", " ", "frequency"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"\[CapitalGamma]", "=", 
   RowBox[{"\[CapitalGamma]D2", "/", "2"}]}], " ", ";", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "they", " ", "are", " ", "using", " ", "87", "Rb", " ", "D2", " ", 
     "line"}], ",", " ", 
    RowBox[{
     RowBox[{
     "and", " ", "their", " ", "\[CapitalGamma]", " ", "is", " ", "the", " ", 
      "half"}], "-", "width"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"\[Tau]", "=", 
   SuperscriptBox["10", 
    RowBox[{"-", "5"}]]}], ";", 
  RowBox[{"(*", 
   RowBox[{"measurement", " ", "interval"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"steps", "=", 
   RowBox[{"2", "\[Pi]", "*", 
    SuperscriptBox["10", "6"], 
    RowBox[{"{", 
     RowBox[{"6", ",", "20", ",", "50", ",", "100", ",", "200", ",", "500"}], 
     "}"}]}]}], ";", 
  RowBox[{"(*", 
   RowBox[{"\[Kappa]lo", " ", "steps", " ", "in", " ", 
    SuperscriptBox["s", 
     RowBox[{"-", "1"}]]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"data", " ", "=", " ", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "<", 
    RowBox[{
     RowBox[{"Length", "[", "steps", "]"}], "+", "1"}]}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Kappa]lo", "=", 
     RowBox[{"steps", "[", 
      RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Kappa]T", " ", "=", " ", 
     RowBox[{"\[Kappa]lo", "/", "2"}]}], ";", 
    RowBox[{
    "(*", "\"\<mirror transmission rate was chosen to be optimal for every \
curve\>\"", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"\[Kappa]", "=", 
     RowBox[{"\[Kappa]T", "+", "\[Kappa]lo"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"data", ",", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"jIn", ",", 
          RowBox[{
           RowBox[{
            SqrtBox[
             RowBox[{"jIn", " ", "*", 
              SuperscriptBox["10", "6"], "\[Tau]", " "}]], 
            FractionBox["\[Kappa]T", "\[Kappa]"], 
            RowBox[{"(", 
             RowBox[{"1", "+", 
              FractionBox["\[Gamma]", "\[Kappa]"], "-", 
              FractionBox["1", 
               RowBox[{"1", "+", 
                FractionBox["\[Gamma]", "\[Kappa]"]}]]}], ")"}]}], "/.", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\[CapitalDelta]a", "\[Rule]", " ", "0"}], ",", 
             RowBox[{"\[CapitalDelta]c", "\[Rule]", " ", "0"}], ",", 
             RowBox[{"n", "\[Rule]", 
              RowBox[{
               RowBox[{"NSolve", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"n", "==", 
                   FractionBox[
                    SuperscriptBox["\[Eta]", "2"], 
                    RowBox[{
