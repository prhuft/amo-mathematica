afdd68302567"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"SPAD", " ", "entanglement", " ", "rate", " ", 
    RowBox[{"calculation", ".", " ", "light"}], " ", "is", " ", "coupled", 
    " ", "to", " ", "the", " ", "detector", " ", "in", " ", "free", " ", 
    "space"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"NA", " ", "=", " ", "0.55"}], ";", " ", 
    RowBox[{"(*", 
     RowBox[{
     "the", " ", "highest", " ", "NA", " ", "JenOptik", " ", "objectives", 
      " ", "we", " ", "have"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"\[Theta]lens", " ", "=", 
     RowBox[{"ArcSin", "[", "NA", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Eta]lens", "=", 
     RowBox[{"norm", " ", 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"lh", ".", 
          RowBox[{"conj", "[", "lh", "]"}]}], 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "0", ",", "\[Theta]lens"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Phi]", ",", "0", ",", 
          RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "quantum", " ", "efficiency", " ", "for", " ", "sigma", " ", "light"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Eta]detect", " ", "=", "0.15"}], ";", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"SPAD", " ", "eff", " ", "at", " ", "780"}], ",", " ", 
      RowBox[{
       RowBox[{"e", ".", "g", ".", " ", "the"}], " ", "Pi", " ", "Imaging", 
       " ", 
       SuperscriptBox["SPAD512", "2"]}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"Paa", " ", "=", " ", 
     RowBox[{"0.5", "*", 
      SuperscriptBox["\[Eta]detect", "2"], 
      SuperscriptBox["\[Eta]lens", "2"]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"ntotal", " ", "=", 
     RowBox[{"Floor", "[", " ", 
      RowBox[{
       FractionBox["1", "Paa"], "+", "0.5"}], "]"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
      "total", " ", "number", " ", "of", " ", "tries", " ", "to", " ", "get", 
       " ", "one", " ", "atom"}], "-", 
      RowBox[{
      "atom", " ", "entangled", " ", "pair", " ", "on", " ", "average"}]}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{"nn1", "=", "15"}], ";", " ", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"tries", " ", "without", " ", "needing", " ", "to", " ", "re"}],
       "-", 
      RowBox[{"cool", " ", "the", " ", 
       RowBox[{"atom", "/", 
        RowBox[{"ion", ".", " ", "not"}]}], " ", "justified", " ", 
       RowBox[{"here", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"nn", " ", "=", " ", 
     RowBox[{"Floor", "[", 
      RowBox[{
       FractionBox["ntotal", "nn1"], "+", "0.5"}], "]"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{"epoch", " ", "=", " ", 
      RowBox[{
      "nn1", " ", "trials", " ", "plus", " ", "a", " ", "cooling", " ", 
       "phase"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"Tcool", " ", "=", " ", 
     RowBox[{"0.1", "*", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]]}]}], ";", " ", 
    RowBox[{"(*", 
     RowBox[{"times", " ", "in", " ", "s"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"T\[Pi]", " ", "=", " ", "30*^-9"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Tdet", "=", "1*^-6"}], ";"}], " ", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Tpump", "=", "6*^-6"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"TMOT", " ", "=", "0.5"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Tentangle", " ", "=", " ", 
     RowBox[{"nn", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"nn1", 
         RowBox[{"(", 
          RowBox[{"T\[Pi]", "+", "Tdet", "+", "Tpump"}], ")"}]}], " ", "+", 
        " ", "Tcool"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Nentangles", "=", 
     RowBox[{"Tlifetime", "/", "Tentangle"}]}], ";"}], "\[IndentingNewLine]", 
   "\"\<average attempt rate (incl. MOT/FORT loading)\>\"", 
   "\[IndentingNewLine]", 
   FractionBox["1", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"T\[Pi]", "+", "Tdet", "+", "Tpump"}], ")"}], " ", "+", " ", 
     RowBox[{"Tcool", "/", "nn1"}], "+", 
     RowBox[{"TMOT", "/", "Nentangles"}]}]], 
   RowBox[{"(*", 
    RowBox[{"note", ":", " ", 
     RowBox[{
     "Akbar", " ", "used", " ", "40", "kHz", " ", "based", " ", "on", " ", 
      "what", " ", "Weinfurter", " ", "and", " ", "Rempe", " ", "have", " ", 
      "done"}]}], "*)"}], "\[IndentingNewLine]", 
   "\"\<average attempt rate (not incl. MOT/FORT loading)\>\"", 
   "\[IndentingNewLine]", 
   FractionBox["1", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"T\[Pi]", "+", "Tdet", "+", "Tpump"}], ")"}], " ", "+", " ", 
     RowBox[{"Tcool", "/", "nn1"}]}]], "\[IndentingNewLine]", 
   "\"\<Paa, high NA lens and free space coupling to SPAD\>\"", 
   "\[IndentingNewLine]", "Paa", "\[IndentingNewLine]", 
   "\"\<expected attempts before atom-atom entanglement event\>\"", 
   "\[IndentingNewLine]", "ntotal", "\[IndentingNewLine]", 
   "\"\<attempts per cooling phase\>\"", "\[IndentingNewLine]", "nn1", 
   "\[IndentingNewLine]", 
   "\"\<epochs (number of nn1 attempts plus a cooling phase)\>\"", 
   "\[IndentingNewLine]", "nn", "\[IndentingNewLine]", 
   "\"\<e-bits/s with mirror (incl. MOT/FORT loading)\>\"", 
   "\[IndentingNewLine]", 
   RowBox[{"1", "/", 
    RowBox[{"(", 
