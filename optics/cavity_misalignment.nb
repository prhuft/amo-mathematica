  RowBox[{"(*", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["10", 
       RowBox[{"-", "i"}]], "mrad"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "4"}], "]"}]}], "}"}]}], "]"}], "*)"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lossTable", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Clear", "[", "\[Theta]", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "<", 
     RowBox[{
      RowBox[{"Length", "[", "tiltTable", "]"}], "+", "1"}]}], ",", 
    RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"mirror", " ", "tilt"}], ",", " ", 
      RowBox[{"rad", "."}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Theta]", "=", 
      RowBox[{"tiltTable", "[", 
       RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dx", "=", 
      RowBox[{
       RowBox[{"R", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Cos", "[", "\[Theta]", "]"}], "-", "1"}], ")"}]}], "+", 
       "d"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dz", "=", 
      RowBox[{"R", " ", 
       RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]", "=", 
      RowBox[{"ArcTan", "[", 
       RowBox[{"dx", ",", "dz"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"mode", " ", "tilt"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"dtilt", "=", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["dx", "2"], "+", 
        SuperscriptBox["dz", "2"]}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Ltilt", " ", "=", 
      RowBox[{
       RowBox[{"2", "R"}], "-", "dtilt"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"cav", " ", "length", " ", "with", " ", "tilt"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"w0", "=", 
      RowBox[{
       SqrtBox[
        FractionBox[
         RowBox[{"\[Lambda]", " ", "L"}], 
         RowBox[{"2", " ", "\[Pi]"}]]], 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{"R", "-", 
           RowBox[{"L", "/", "2"}]}], 
          RowBox[{"L", "/", "2"}]], ")"}], 
        RowBox[{"1", "/", "4"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w0tilt", "=", 
      RowBox[{
       SqrtBox[
        FractionBox[
         RowBox[{"\[Lambda]", " ", "L"}], 
         RowBox[{"2", " ", "\[Pi]"}]]], 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{"R", "-", 
           RowBox[{"Ltilt", "/", "2"}]}], 
          RowBox[{"Ltilt", "/", "2"}]], ")"}], 
        RowBox[{"1", "/", "4"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"zR", "=", 
      RowBox[{"\[Pi]", " ", 
       RowBox[{
        SuperscriptBox["w0", "2"], "/", "\[Lambda]"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"Rayleigh", " ", "range"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"cav", " ", "waist", " ", "with", " ", "tilt"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"wMtilt", "=", 
      RowBox[{"w0tilt", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          SuperscriptBox[
           RowBox[{"(", 
            FractionBox[
             RowBox[{"L", "-", "dtilt"}], 
             RowBox[{"2", " ", "zR"}]], ")"}], "2"]}], ")"}], 
        RowBox[{"1", "/", "2"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"wM", "=", 
      RowBox[{"w0tilt", 
