Cell["\<\
The expressions which compute the critical distance for the tilted cavity \
mode might be incorrect. Check them against the ones that Louis Farenci and I \
derived, in my cavity alignment jupyter notebook.

Assume one mirror is tilted about an axis perpendicular to the optical axis \
and bisecting its surface. The axis of the supported cavity mode tilts as \
well (a nice diagram in Stamper-Kurn Julian thesis) and if it is severe there \
can be diffractive loss from scattering off the edge of a mirror. Below is an \
approximate calculation of this (algebra in my orange Rhodia dot pad).  \
\>", "Text",
 CellChangeTimes->{{3.865200577403032*^9, 3.8652008124747972`*^9}, {
  3.891334563319025*^9, 3.891334571155729*^9}, {3.891334643087084*^9, 
  3.8913347102224283`*^9}},ExpressionUUID->"45b73b5d-c9c3-42f5-88fb-\
538891c155ce"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Lambda]", "=", 
   RowBox[{"780", " ", "nm"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"d", "=", 
    RowBox[{"10", " ", "um"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"d", " ", "critical"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"R", "=", 
    RowBox[{"2", "mm"}]}], ";"}], "//", "N"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"L", "=", 
    RowBox[{
     RowBox[{"2", "R"}], "-", "d"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"cavity", " ", "length"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"r", "=", 
   RowBox[{"0.5", " ", "mm"}]}], 
  RowBox[{"(*", 
   RowBox[{"clear", " ", "aperture", " ", "radius"}], 
   "*)"}]}], "\[IndentingNewLine]", "\"\<cavity params R, C.A. radius \
[mm]\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"R", ",", "r"}], "}"}], "/", "mm"}], "//", 
  "N"}], "\[IndentingNewLine]", "\"\<tilt [mrad]\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Theta]", "=", 
    RowBox[{".1", " ", "mrad"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"mirror", " ", "tilt"}], ",", " ", 
    RowBox[{"rad", "."}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]", " ", "/", "mrad"}], "//", "N"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dx", "=", 
   RowBox[{
    RowBox[{"R", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Cos", "[", "\[Theta]", "]"}], "-", "1"}], ")"}]}], "+", 
    "d"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dz", "=", 
   RowBox[{"R", " ", 
    RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Alpha]", "=", 
    RowBox[{"ArcTan", "[", 
     RowBox[{"dx", ",", "dz"}], "]"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"mode", " ", "tilt"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dtilt", "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["dx", "2"], "+", 
     SuperscriptBox["dz", "2"]}]]}], 
  ";"}], "\[IndentingNewLine]", "\"\<critical distance [\[Mu]m] with/without \
tilt\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"dtilt", ",", "d"}], "}"}], "/", "um"}], "//", 
  "N"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Ltilt", " ", "=", 
    RowBox[{
     RowBox[{"2", "R"}], "-", "dtilt"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"cav", " ", "length", " ", "with", " ", "tilt"}], 
   "*)"}]}], "\[IndentingNewLine]", "\"\<cavity mode waist [\[Mu]m], \
with/without tilt\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"w0", "=", 
   RowBox[{
    SqrtBox[
     FractionBox[
      RowBox[{"\[Lambda]", " ", "L"}], 
      RowBox[{"2", " ", "\[Pi]"}]]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"R", "-", 
        RowBox[{"L", "/", "2"}]}], 
       RowBox[{"L", "/", "2"}]], ")"}], 
     RowBox[{"1", "/", "4"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"w0tilt", "=", 
   RowBox[{
    SqrtBox[
     FractionBox[
      RowBox[{"\[Lambda]", " ", "L"}], 
      RowBox[{"2", " ", "\[Pi]"}]]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"R", "-", 
        RowBox[{"Ltilt", "/", "2"}]}], 
       RowBox[{"Ltilt", "/", "2"}]], ")"}], 
     RowBox[{"1", "/", "4"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"w0", ",", "w0tilt"}], "}"}], "/", "um"}], "//", 
  "N"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"zR", "=", 
    RowBox[{"\[Pi]", " ", 
     RowBox[{
      SuperscriptBox["w0", "2"], "/", "\[Lambda]"}]}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"Rayleigh", " ", "range"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"zRtilt", " ", "=", 
   RowBox[{"\[Pi]", " ", 
    RowBox[{
     SuperscriptBox["w0tilt", "2"], "/", "\[Lambda]"}]}]}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"cav", " ", "waist", " ", "with", " ", "tilt"}], 
   "*)"}]}], "\[IndentingNewLine]", "\"\<mode waist at mirror [\[Mu]m], \
with/without tilt\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"wMtilt", "=", 
   RowBox[{"w0tilt", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{
           RowBox[{"2", "R"}], "-", "dtilt"}], 
          RowBox[{"2", " ", "zR"}]], ")"}], "2"]}], ")"}], 
     RowBox[{"1", "/", "2"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"wM", "=", 
   RowBox[{"w0tilt", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{
           RowBox[{"2", "R"}], "-", "d"}], 
          RowBox[{"2", " ", "zRtilt"}]], ")"}], "2"]}], ")"}], 
     RowBox[{"1", "/", "2"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{"wMtilt", ",", "wM"}], "}"}], "/", "um"}], "//", "N"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Clear", "[", 
     RowBox[{
     "\[Theta]", ",", "d", ",", "L", ",", "R", ",", "r", ",", "\[Alpha]", ",",
       "w"}], "]"}], ";"}], 
   "*)"}]}], "\[IndentingNewLine]", "\"\<displacement of mode on mirror \
[mm]\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x0", "=", 
   RowBox[{"R", " ", 
    RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ";", 
  RowBox[{"(*", 
   RowBox[{"this", " ", "is", " ", "not", " ", "quite", " ", "correct"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"x0", "/", "mm"}], "//", "N"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mode", "=", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"2", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"x", "-", "x0"}], ")"}], "2"], "+", 
         SuperscriptBox["y", "2"]}], ")"}]}], 
      SuperscriptBox["wMtilt", "2"]]}], "]"}]}], ";", 
  RowBox[{"(*", 
   RowBox[{"mode", " ", "on", " ", "the", " ", "mirror"}], "*)"}], 
  "\[IndentingNewLine]", 
  "\"\<ppm diffractive loss per side\>\""}], "\[IndentingNewLine]", 
 RowBox[{"ppmLoss", "=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["10", "6"], 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{
       RowBox[{"Integrate", "[", 
        RowBox[{"mode", ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "r"}], ",", "r"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"y", ",", 
           RowBox[{"-", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["r", "2"], "-", 
              SuperscriptBox["x", "2"]}]]}], ",", 
           SqrtBox[
            RowBox[{
             SuperscriptBox["r", "2"], "-", 
             SuperscriptBox["x", "2"]}]]}], "}"}]}], "]"}], "/", 
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]", " ", 
         RowBox[{"Integrate", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "2"}], 
              FractionBox[
               SuperscriptBox["\[Rho]", "2"], 
               SuperscriptBox["wM", "2"]]}], "]"}], "\[Rho]"}], ",", 
           RowBox[{"{", 
            RowBox[{"\[Rho]", ",", "0", ",", "\[Infinity]"}], "}"}]}], 
          "]"}]}], ")"}]}]}], ")"}]}], "//", "N"}]}]}], "Input",
