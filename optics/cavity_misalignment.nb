     RowBox[{"(*", 
      RowBox[{"mode", " ", "on", " ", "the", " ", "mirror"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"lossTable", ",", 
       RowBox[{
        RowBox[{
         SuperscriptBox["10", "6"], 
