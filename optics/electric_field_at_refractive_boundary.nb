            FractionBox["\[Pi]", "2"]}]]}], "}"}], ".", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"rp", "/.", 
            RowBox[{"\[Theta]", "->", 
             RowBox[{"(", 
              RowBox[{"norm", "-", 
               RowBox[{"ArcTan", "[", 
                RowBox[{
                 RowBox[{"f", "-", "x"}], ",", "z"}], "]"}]}], ")"}]}]}], " ",
            ")"}], ",", " ", 
