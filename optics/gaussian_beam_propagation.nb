 RowBox[{
  RowBox[{"beamPropagate", "[", 
   RowBox[{"qInit_", ",", "sys_", ",", "color_", ",", "legend_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"q", "=", "qInit"}], ",", "qNext", ",", 
