              RowBox[{"z", "<", "x", "<", "zNext"}]}], "}"}], "}"}], "]"}]}], 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"q", "=", 
         RowBox[{"qNext", "+", 
          RowBox[{"element", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"z", "=", "zNext"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", " ", 
     RowBox[{"(*", 
      RowBox[{"waist", " ", 
       RowBox[{"in", " ", "[", "m", "]"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Plot", "[", 
      RowBox[{"plots", ",", 
